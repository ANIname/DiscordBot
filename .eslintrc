{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": [
    "immutable", "import", "no-use-extend-native", "node", "optimize-regex", "promise", "sonarjs", "unicorn"
  ],
  "rules": {
    "no-console": [2, {"allow": ["info", "warn", "error"]}],
    "global-require": 0,
    "no-use-before-define": 0,
    "import/no-dynamic-require": 0,
    "consistent-return": 0
  }
}